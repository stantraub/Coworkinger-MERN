{
  "name": "coworking-project",
  "version": "1.0.0",
  "description": "coworking space aggregator",
  "main": "index.js",
  "scripts": {
    "server:debug": "nodemon --inspect app.js",
    "server": "nodemon app.js",
    "start": "node app.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://stantraub@bitbucket.org/stantraub/coworking-project.git"
  },
  "author": "Stanley Traub",
  "license": "ISC",
  "homepage": "https://bitbucket.org/stantraub/coworking-project#readme",
  "dependencies": {
    "aws-sdk": "^2.643.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "concurrently": "^5.0.2",
    "express": "^4.18.2",
    "file-type": "^14.1.4",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multiparty": "^4.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "uuid": "^7.0.3",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
